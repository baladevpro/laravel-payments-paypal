{
  "name": "baladev-pro/laravel-payments-paypal",
  "type": "library",
  "description": "Laravel plugin For Processing Payments Through Paypal Express Checkout. Can Be Used Independently With Other Applications.",
  "version": "1.0",
  "keywords": ["http", "rest", "web service", "paypal", "laravel paypal"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Baladev",
      "email": "baladevpro@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BaladevPro\\PayPal\\": "src/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5|~5.6",
    "nesbot/carbon": "~1.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BaladevPro\\PayPal\\Providers\\PayPalServiceProvider"
      ],
      "aliases": {
        "PayPal": "BaladevPro\\PayPal\\Facades\\PayPal"
      }      
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
